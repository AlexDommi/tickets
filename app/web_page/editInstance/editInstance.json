      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.instancias ){",
        "\treturn {",
        "\t\t//map instancias variable to expected task contract input",
        "\t\tinstanciasInput: {",
        "\t\t\tinstancia_codigo_k: $data.instancias.instancia_codigo_k !== undefined ? $data.instancias.instancia_codigo_k : null,",
        "\t\t\tinstancia_udn: $data.instancias.instancia_udn !== undefined ? $data.instancias.instancia_udn : null,",
        "\t\t\tinstancia_descripcion: $data.instancias.instancia_descripcion !== undefined ? $data.instancias.instancia_descripcion : null",
